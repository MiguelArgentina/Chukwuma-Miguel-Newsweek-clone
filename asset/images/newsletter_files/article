var margin_top = 65;
function fitPlayback() {
  $('#playback').css({
    height: (window.innerHeight - margin_top) + 'px',
    top: margin_top
  });
}
fitPlayback();
$(window).on('resize', function() {
  fitPlayback(); 
});
$('#wm-tb-close').click(function(){
  margin_top = 0;
  fitPlayback();
});
